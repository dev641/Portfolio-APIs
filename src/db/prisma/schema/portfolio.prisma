model Portfolio {
  id          Int         @id @default(autoincrement())
  userId      String
  userProfile UserProfile @relation(fields: [userId], references: [userId])
  heading     String
  skillSet    Logo[]
  likes       Int?
  feature     String
  description String
  link        Link?       @relation(fields: [linkId], references: [portfolioLinkId])
  linkId      Int?        @unique
}
