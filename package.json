{
  "name": "portfolio-apis",
  "version": "1.0.0",
  "description": "Portfolio-APIs for Portfolio",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node public/server.js",
    "dev": "nodemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:migrate": "prisma migrate dev --schema=src/db/prisma/schema",
    "prisma:generate": "prisma generate --schema=src/db/prisma/schema",
    "prisma:format": "npx prisma format --schema=src/db/prisma/schema",
    "prisma:reset": "npx prisma migrate reset --schema=src/db/prisma/schema"
  },
  "author": "Devashish",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "@vercel/node": "^3.2.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.8",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "^5.16.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  }
}
